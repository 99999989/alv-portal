{
  "name": "alv-portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run i18n && npm run start:dev",
    "start:dev": "ng serve --base-href / --watch --proxy-config proxy.conf.json",
    "start:prod": "ng serve --base-href / --watch --prod --aot --source-map --proxy-config proxy.conf.json",
    "build": "npm run i18n && npm run build:dev",
    "build:dev": "ng build --base-href /alv-portal-webapp/",
    "build:prod": "ng build --base-href /alv-portal-webapp/ --aot --prod",
    "test": "ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "npm cache verify && rimraf -- node_modules doc target/dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "postinstall": "node version.js",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr,de,it}.json --clean --sort --format namespaced-json --format-indentation ' ' ",
    "i18n": "node pipeline-scripts/csv2json-i18n.js translations.csv --output src/assets/i18n"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.9",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "@fortawesome/fontawesome-free": "5.3.1",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@fortawesome/fontawesome-svg-core": "1.2.4",
    "@fortawesome/free-solid-svg-icons": "5.3.1",
    "@ng-bootstrap/ng-bootstrap": "3.3.1",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@types/pretty-bytes": "5.1.0",
    "angular2-prettyjson": "3.0.1",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "ng2-file-upload": "1.3.0",
    "ngx-cookie-service": "1.0.10",
    "pretty-bytes": "5.1.0",
    "rxjs": "6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.3",
    "@angular/cli": "6.2.3",
    "@angular/compiler-cli": "6.1.9",
    "@angular/language-service": "6.1.9",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@compodoc/compodoc": "1.1.5",
    "@types/jasmine": "2.8.8",
    "@types/jasmine-matchers": "0.2.31",
    "@types/jasminewd2": "2.0.4",
    "@types/node": "10.5.1",
    "codelyzer": "4.4.2",
    "cross-var": "1.1.0",
    "crossenv": "0.0.2-security",
    "fs-extra": "7.0.0",
    "git-describe": "4.0.3",
    "jasmine-core": "3.2.1",
    "jasmine-expect": "3.8.4",
    "jasmine-spec-reporter": "4.2.1",
    "json-stable-stringify": "https://github.com/ganqqwerty/json-stable-stringify",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.3.1",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "papaparse": "4.6.1",
    "protractor": "5.4.1",
    "rimraf": "2.6.2",
    "rxjs-tslint-rules": "4.8.0",
    "source-map-explorer": "1.6.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-angular": "1.1.2",
    "typescript": "2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alv-ch/alv-portal.git"
  },
  "//": [
    "rimraf, crossenv and crossvar are needed for cross-browser compatibility of the npm scripts (including Windows)"
  ],
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 6"
  }
}
