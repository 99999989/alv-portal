{
  "name": "alv-portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "npm run i18n && ng serve --base-href / --watch --proxy-config proxy.conf.json",
    "start:prod": "npm run i18n && ng serve --base-href / --watch --prod --aot --source-map --proxy-config proxy.conf.json",
    "build": "npm run i18n && npm run build:dev",
    "build:dev": "npm run i18n && ng build --base-href /alv-portal-webapp/",
    "build:prod": "npm run i18n && ng build --base-href /alv-portal-webapp/ --aot --prod --stats-json",
    "test": "ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "npm cache verify && rimraf -- node_modules doc target/classes",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "postinstall": "node version.js",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr,de,it}.json --clean --sort --format namespaced-json --format-indentation ' ' ",
    "i18n": "node pipeline-scripts/csv2json-i18n.js translations.csv --output src/assets/i18n",
    "bundle-report": "webpack-bundle-analyzer target/classes/ch/admin/seco/alvportal/ui/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.1",
    "@angular/common": "7.2.1",
    "@angular/compiler": "7.2.1",
    "@angular/core": "7.2.1",
    "@angular/forms": "7.2.1",
    "@angular/http": "7.2.1",
    "@angular/platform-browser": "7.2.1",
    "@angular/platform-browser-dynamic": "7.2.1",
    "@angular/router": "7.2.1",
    "@fortawesome/fontawesome-free": "5.6.3",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@fortawesome/fontawesome-svg-core": "1.2.12",
    "@fortawesome/free-solid-svg-icons": "5.6.3",
    "@ng-bootstrap/ng-bootstrap": "4.0.1",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "7.0.0",
    "@ngrx/router-store": "7.0.0",
    "@ngrx/store": "7.0.0",
    "@ngrx/store-devtools": "7.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/dom-inputevent": "1.0.3",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.119",
    "@types/lodash-es": "4.17.1",
    "@types/pretty-bytes": "5.1.0",
    "angular2-prettyjson": "3.0.1",
    "bootstrap": "4.2.1",
    "core-js": "2.6.2",
    "i18n-iso-countries": "3.7.8",
    "jwt-decode": "2.2.0",
    "libphonenumber-js": "1.7.7",
    "ng2-file-upload": "1.3.0",
    "ng2-nouislider": "1.7.13",
    "ngx-clipboard": "11.1.9",
    "ngx-cookie-service": "2.1.0",
    "ngx-gravatar": "7.2.0",
    "ngx-infinite-scroll": "7.0.1",
    "ngx-markdown": "7.1.1",
    "nodelist-foreach-polyfill": "1.2.0",
    "nouislider": "12.1.0",
    "open-sans-fonts": "1.5.0",
    "pretty-bytes": "5.1.0",
    "rxjs": "6.3.3",
    "zone.js": "0.8.28"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.2",
    "@angular/cli": "7.2.2",
    "@angular/compiler-cli": "7.2.1",
    "@angular/language-service": "7.2.1",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@compodoc/compodoc": "1.1.7",
    "@types/jasmine": "3.3.5",
