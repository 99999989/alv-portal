{
  "name": "online-services-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:dev",
    "start:dev": "ng serve --watch",
    "start:prod": "ng serve --watch --prod --aot --source-map",
    "build": "npm run build:dev",
    "build:dev": "ng build",
    "build:prod": "ng build --aot --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "npm cache verify && rimraf -- node_modules doc target/dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.7",
    "@angular/common": "6.0.7",
    "@angular/compiler": "6.0.7",
    "@angular/core": "6.0.7",
    "@angular/forms": "6.0.7",
    "@angular/http": "6.0.7",
    "@angular/platform-browser": "6.0.7",
    "@angular/platform-browser-dynamic": "6.0.7",
    "@angular/router": "6.0.7",
    "@fortawesome/fontawesome-free": "5.2.0",
    "@fortawesome/fontawesome-free-webfonts": "1.0.9",
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@ng-bootstrap/ng-bootstrap": "2.2.0",
    "@types/pretty-bytes": "5.1.0",
    "angular2-prettyjson": "3.0.1",
    "bootstrap": "4.1.1",
    "core-js": "2.5.7",
    "ng2-file-upload": "1.3.0",
    "pretty-bytes": "5.1.0",
    "rxjs": "6.2.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.7",
    "@angular/language-service": "6.0.7",
    "@compodoc/compodoc": "1.1.3",
    "@types/jasmine": "2.8.8",
    "@types/jasmine-matchers": "0.2.30",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.1",
    "codelyzer": "4.4.2",
    "cross-var": "1.1.0",
    "crossenv": "0.0.2-security",
    "jasmine-core": "3.1.0",
    "jasmine-expect": "3.8.3",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "protractor": "5.3.2",
    "rimraf": "2.6.2",
    "source-map-explorer": "1.5.0",
    "ts-node": "7.0.0",
    "tslint": "5.10.0",
    "tslint-angular": "1.1.2",
    "typescript": "2.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alv-ch/online-services.git"
  },
  "//": [
    "rimraf, crossenv and crossvar are needed for cross-browser compatibility of the npm scripts (including Windows)"
  ],
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 6"
  }
}
